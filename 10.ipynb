   "source": [
    "def cyclic_shift(words, n):\n",
    "   words = words.split()\n",
    "   n = n % len(words)\n",
    "   return ' '.join(words[n:] + words[:n])\n",
    "stroka=input('Введите строку ')\n",
    "n=int(input('Введите длину сдвига '))\n",
    "print(cyclic_shift(stroka, n))"
   ]
